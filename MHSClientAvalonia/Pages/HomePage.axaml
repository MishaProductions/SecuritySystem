<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="MHSClientAvalonia.Pages.HomePage">
	<ScrollViewer>
		<Grid>
			<StackPanel Spacing="4">
				<ContentControl>
					<StackPanel>
						<TextBlock x:Name="zone1" FontSize="20">Loading...</TextBlock>
						<TextBlock x:Name="zone2" FontSize="20">Loading...</TextBlock>
						<TextBlock x:Name="zone3" FontSize="20">Loading...</TextBlock>
						<TextBlock x:Name="zone4" FontSize="20">Loading...</TextBlock>
						<TextBlock x:Name="zone5" FontSize="20">Loading...</TextBlock>
						<TextBlock x:Name="zone6" FontSize="20">Loading...</TextBlock>
						<TextBlock x:Name="zone7" FontSize="20">Loading...</TextBlock>
						<TextBlock x:Name="zone8" FontSize="20">Loading...</TextBlock>
						<TextBlock x:Name="zone9" FontSize="20">Loading...</TextBlock>
						<TextBlock x:Name="zone10" FontSize="20">Loading...</TextBlock>
					</StackPanel>
				</ContentControl>

				<SelectableTextBlock x:Name="weatherInfoShort">Loading weather information...</SelectableTextBlock>
			</StackPanel>
		</Grid>
	</ScrollViewer>
</UserControl>
