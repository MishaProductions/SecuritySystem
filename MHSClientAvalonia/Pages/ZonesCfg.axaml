<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="MHSClientAvalonia.Pages.ZonesCfg">
	<WrapPanel Orientation="Vertical" Margin="5">
		<WrapPanel x:Name="ZonesContainer" Orientation="Vertical">
		
		</WrapPanel>

		<WrapPanel>
			<Button Margin="5" Click="Save_Click"  Classes="accent">Save</Button>
			<Button Margin="5" Click="Cancel_Click">Cancel</Button>
		</WrapPanel>
	</WrapPanel>
</UserControl>
