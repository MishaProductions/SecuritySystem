<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="MHSClientAvalonia.ChangePermissionView">
	<Grid>
		<Grid.ColumnDefinitions>
			<ColumnDefinition Width="auto"></ColumnDefinition>
			<ColumnDefinition Width="*"></ColumnDefinition>
		</Grid.ColumnDefinitions>
		
		<Label x:Name="lblPerm" Grid.Column="0" VerticalAlignment="Center">Permissions:</Label>
		<ComboBox x:Name="cmbPermissions" Grid.Column="2" SelectedIndex="0" Margin="5">
			<ComboBoxItem>User</ComboBoxItem>
			<ComboBoxItem>Administrator</ComboBoxItem>
		</ComboBox>
	</Grid>
</UserControl>
